{
  "name": "c-factor-backend",
  "version": "0.1.0",
  "description": "Node.js REST API backend code for c-factor project",
  "repository": {
    "type": "git",
    "url": "https://github.com/thetalentgames/c-factor-backend"
  },
  "author": "Narain Sagar",
  "license": "MIT",
  "scripts": {
    "postinstall": "npm i -g typescript nodemon tslint concurrently jest npm-run-all",
    "tsc": "tsc",
    "jest": "jest",
    "nodemon": "nodemon",
    "concurrently": "concurrently",
    "npm-run-all": "npm-run-all",
    "copy-assets": "ts-node tools/copy-assets",
    "dev": "nodemon --watch src -e ts,ejs --exec npm run start",
    "start": "npm-run-all build serve",
    "build:clean": "rm -rf dist",
    "build": "npm-run-all build:clean build-ts tslint copy-assets",
    "serve": "node --inspect dist/server.js",
    "watch-node": "nodemon dist/server.js",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run watch-node\"",
    "test": "jest --forceExit --coverage --verbose",
    "build-ts": "tsc",
    "watch-ts": "tsc -w",
    "tslint": "tslint -c tslint.json -p tsconfig.json",
    "tslint:fix": "tslint -c tslint.json -p tsconfig.json --fix",
    "debug": "npm-run-all build watch-debug",
    "serve-debug": "nodemon --inspect dist/server.js",
    "watch-debug": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run serve-debug\""
  },
  "pre-commit": [
    "tslint"
  ],
  "dependencies": {
    "async": "^2.6.1",
    "bcrypt": "^3.0.6",
    "bcrypt-nodejs": "0.0.3",
    "bluebird": "^3.5.3",
    "body-parser": "^1.18.3",
    "compression": "^1.7.3",
    "connect-flash": "^0.1.1",
    "cors": "^2.8.5",
    "dotenv": "^4.0.0",
    "ejs": "^2.6.2",
    "errorhandler": "^1.5.0",
    "express": "^4.16.4",
    "express-jwt": "^5.3.0",
    "express-session": "^1.15.6",
    "express-validator": "^4.3.0",
    "jsonwebtoken": "^8.4.0",
    "lodash": "^4.17.11",
    "lusca": "^1.6.1",
    "morgan": "^1.9.1",
    "nodemailer": "^4.7.0",
    "passport": "^0.4.0",
    "passport-linkedin-oauth2": "^2.0.0",
    "pg": "^7.12.1",
    "pg-hstore": "^2.3.3",
    "pug": "^2.0.4",
    "ramda": "^0.26.1",
    "request": "^2.88.0",
    "sequelize": "^5.15.1",
    "swagger-ui-express": "^3.0.10"
  },
  "devDependencies": {
    "@types/async": "^2.0.50",
    "@types/bcrypt-nodejs": "0.0.30",
    "@types/bluebird": "^3.5.25",
    "@types/body-parser": "^1.17.0",
    "@types/compression": "0.0.33",
    "@types/dotenv": "^4.0.3",
    "@types/errorhandler": "0.0.32",
    "@types/express": "^4.16.0",
    "@types/express-jwt": "0.0.38",
    "@types/express-session": "^1.15.11",
    "@types/jest": "^21.1.8",
    "@types/jsonwebtoken": "^7.2.8",
    "@types/lodash": "^4.14.119",
    "@types/lusca": "^1.5.0",
    "@types/morgan": "^1.7.32",
    "@types/node": "^7.10.2",
    "@types/nodemailer": "^1.3.32",
    "@types/pg": "^7.11.0",
    "@types/request": "^2.48.1",
    "@types/sequelize": "^4.28.4",
    "@types/shelljs": "^0.7.9",
    "@types/supertest": "^2.0.7",
    "chai": "^4.2.0",
    "concurrently": "^3.6.1",
    "jest": "^25.0.0",
    "nodemon": "^1.18.9",
    "npm-run-all": "^4.1.5",
    "pre-commit": "^1.2.2",
    "rimraf": "^3.0.0",
    "shelljs": "^0.7.8",
    "supertest": "^3.3.0",
    "ts-jest": "^24.1.0",
    "ts-node": "^4.0.2",
    "tslint": "^5.11.0",
    "typescript": "^3.5.3"
  }
}
